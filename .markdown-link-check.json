{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "User-Agent": "GitHub-Actions"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 2,
  "fallbackHttpStatus": [400, 401, 403, 404, 429, 500, 502, 503, 504]
}